{
  "name": "was-api",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap --ts test/**/*.test.ts",
    "start": "npm run build:ts -w && fastify start -r tsconfig-paths/register -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P -r tsconfig-paths/register dist/app.js\"",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d ./src/config/typeOrmDatasource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^3.0.2",
    "@fastify/autoload": "^5.1.0",
    "@fastify/cookie": "^6.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/env": "^3.0.0",
    "@fastify/sensible": "^4.1.0",
    "argon2": "^0.28.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "fast-jwt": "^1.5.1",
    "fastify": "^3.0.0",
    "fastify-auth": "^1.2.0",
    "fastify-cli": "^2.13.0",
    "fastify-plugin": "^3.0.0",
    "fastify-typeorm-plugin": "^2.2.0",
    "json-schema-to-ts": "^1.6.5",
    "pg": "^8.7.3",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@types/node": "^15.14.9",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.2.4"
  }
}
