{
  "name": "was-api",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap --ts test/**/*.test.ts",
    "start": "npm run build:ts -w && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.3",
    "date-fns": "^2.28.0",
    "fastify": "^3.0.0",
    "fastify-auth": "^1.1.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.13.0",
    "fastify-cookie": "^5.6.0",
    "fastify-env": "^2.1.1",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^3.0.0",
    "fastify-postgres": "^3.6.0",
    "fastify-sensible": "^3.1.0",
    "fastify-typeorm-plugin": "^2.2.0",
    "json-schema-to-ts": "^1.6.5"
  },
  "devDependencies": {
    "@types/node": "^15.0.0",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "tap": "^15.0.10",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
